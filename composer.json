{
  "name": "cpsit/persons",
  "type": "typo3-cms-extension",
  "description": "Manage persons",
  "authors": [
    {
      "name": "Dirk Wenzel",
      "role": "Developer"
    },
    {
      "name": "Elias Häußler",
      "email": "e.haeussler@familie-redlich.de",
      "role": "Developer"
    }
  ],
  "keywords": [
    "TYPO3 CMS",
    "Person"
  ],
  "homepage": "https://github.com/CPS-IT/persons",
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/minimal": "^9.5 | ^10.4 | ^11.5",
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "nimut/testing-framework": "^5.3",
    "phpunit/phpunit": "^7",
    "typo3/testing-framework": "*"
  },
  "autoload": {
    "psr-4": {
      "CPSIT\\Persons\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CPSIT\\Persons\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "persons",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
