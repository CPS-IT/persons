{
  "name": "cpsit/persons",
  "type": "typo3-cms-extension",
  "description": "Manage persons",
  "author": [
    {
      "name": "Dirk Wenzel",
      "role": "Developer"
    }
  ],
  "keywords": [
    "TYPO3 CMS",
    "Person"
  ],
  "homepage": "https://github.com/CPS-IT/persons",
  "license": [
    "GPL-2.0+"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/cms-core": "^8.7.1"
  },
  "require-dev": {
    "nimut/testing-framework": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "CPSIT\\Persons\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CPSIT\\Persons\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
